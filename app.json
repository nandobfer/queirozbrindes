{
  "expo": {
    "name": "Queiroz Brindes",
    "slug": "queirozbrindes",
    "owner": "nandoburgos",
    "scheme": "queirozbrindes",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "light",
      "supportsTablet": false,
      "bundleIdentifier": "dev.nandoburgos.queirozbrindes",
      "associatedDomains": [
        "applinks:app.queirozbrindes.com.br"
      ]
    },
    "android": {
      "userInterfaceStyle": "light",
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground-icon.png",
        "monochromeImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "softwareKeyboardLayoutMode": "resize",
      "package": "dev.nandoburgos.queirozbrindes",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#fff"
    },
    "plugins": [
      [
        "./plugins/withDisableForcedDarkModeAndroid.js",
        {}
      ],
      [
        "./plugins/withAndroidLargeHeap.js",
        {}
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you attach any to your orders."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/custom/FoundersGroteskBOZ-Regular.otf",
            "assets/fonts/custom/FoundersGroteskBOZ-Bold.otf",
            "assets/fonts/custom/FoundersGroteskXCondBOZ-Bold.otf",
            "assets/fonts/FoundersGrotesk-Regular.otf",
            "assets/fonts/FoundersGrotesk-Bold.otf",
            "assets/fonts/FoundersGroteskXCond-Bold.otf"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#fff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c8a442eb-262a-4278-9d54-c779068e3046"
      }
    }
  }
}
